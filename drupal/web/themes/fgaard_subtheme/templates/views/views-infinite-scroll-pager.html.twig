{#
/**
 * @file
 * The views infinite scroll pager template.
 */
#}

{% if items.next %}
  <ul{{ attributes }}>
    <li class="pager__item">
      <a class="button" href="{{ items.next.href }}" title="{{ 'Go to next page'|t }}" rel="next"><span class="glyphicon glyphicon-spin glyphicon-adjust"></span> Loading</a>
    </li>
  </ul>
{% endif %}